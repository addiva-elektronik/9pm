# This is a default running config for 9pm.
#
# If you wish to have your own configuration you can copy this to
# ~/.9pm.rc and modify the values as you see fit.

# Default ssh options passed to ssh and scp
SSH_OPTS: "-o StrictHostKeyChecking=no"


# Default console (conserver) options
# CONSOLE_OPTS: "--example"

# Default log base path
LOG_PATH: "~/9pm_log"
